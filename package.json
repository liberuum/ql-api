    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/pg": "^8.6.5",
    "nodemon": "^2.0.16",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  }
}
